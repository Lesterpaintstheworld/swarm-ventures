{
    "version": 2,
    "builds": [
        {
            "src": "api/*.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "/api/index.py"
        },
        {
            "src": "/api/(.*)",
            "dest": "/api/$1.py"
        }
    ]
}
