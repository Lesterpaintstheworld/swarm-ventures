{
    "version": 2,
    "builds": [
        {
            "src": "api/*.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.9"
            }
        },
        {
            "src": "package.json",
            "use": "@vercel/next"
        }
    ],
    "routes": [
        {
            "src": "/premium",
            "dest": "/premium"
        },
        {
            "src": "/api/(.*)",
            "dest": "/api/$1"
        }
    ]
}
