{
    "version": 2,
    "builds": [
        {
            "src": "api/*.py",
            "use": "@vercel/python"
        },
        {
            "src": "package.json",
            "use": "@vercel/next"
        }
    ],
    "routes": [
        {
            "src": "/premium",
            "dest": "/premium"
        },
        {
            "src": "/api/(.*)",
            "dest": "/api/$1"
        }
    ]
}
