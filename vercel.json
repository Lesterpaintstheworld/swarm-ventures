{
    "version": 2,
    "builds": [
        {
            "src": "api/telegram.py",
            "use": "@vercel/python"
        },
        {
            "src": "templates/index.html",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/telegram.py"
        },
        {
            "src": "/(.*)",
            "dest": "/templates/index.html"
        }
    ]
}
