{
    "version": 2,
    "functions": {
        "api/telegram.py": {
            "memory": 1024,
            "maxDuration": 10,
            "runtime": "@vercel/python@3.1.0"
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/telegram.py"
        }
    ]
}
